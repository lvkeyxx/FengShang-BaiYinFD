package com.utility;

import java.util.List;

/**
 * @author wangyg
 * @date 2017-12-04 13:35:01
 * 处理List的相关工具类
 */
public class ListUtil {

	/**
	 * 如果list为空则返回true
	 * list为空两种判断：一是否为null，二是否无值
	 * @param list
	 * @return
	 */
	public static boolean  isEmpty(List list){
		boolean empty=false;
		if(list==null||list.isEmpty()){
			empty=true;
		}
		return empty;
	}
	/**
	 * 如果list不为空，则返回true
	 * list不为空两种判断：一不能为null,二size不能为0；
	 * @param list
	 * @return
	 */
	public static boolean isNotEmpty(List list){
		boolean notEmpty=false;
		if(list!=null&&list.size()>0){
			notEmpty=true;
		}
		return notEmpty;
	}
}
