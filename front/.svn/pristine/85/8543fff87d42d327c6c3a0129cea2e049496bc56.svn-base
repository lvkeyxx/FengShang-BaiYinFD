.problemsSolving{
background-color:#c4c4c4;
.contentCss{
	padding:.3rem;
}
.listCss{
	display:flex;
	justify-content: space-between;
	border-bottom:1px solid gray;
	padding:.2rem 0;
}
.textareaCss{
	width:100%;
	height:2rem;
}
.ideaCss{
	padding:.2rem 0;
	color:green;
	font-size:.4rem;
}
.okCss{
	width: 3rem;
    height: 1rem;
    background-color: green;
    line-height: 1rem;
    text-align: center;
    border-radius: .2rem;
}
.noCss{
	width: 3rem;
    height: 1rem;
    background-color: red;
    line-height: 1rem;
    text-align: center;
    border-radius: .2rem;
}
.buttonCss{
    margin-top:.3rem;
	display:flex;
	justify-content: space-between;
}
.descCss{
	margin-bottom:.1rem;
	
}
.describeAllCss{
	border-bottom:1px solid gray;
	padding:.2rem 0;
}
}