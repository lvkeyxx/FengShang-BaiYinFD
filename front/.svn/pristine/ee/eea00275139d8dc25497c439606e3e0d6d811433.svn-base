.OSIArearCSS {
  @rem: 144rem;
  background-color: #fff;
  .item-icon-left .icon, .item-icon-right .icon {
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
    top: 0;
    height: 100%;
    font-size: 14px;
    color: #ccc;
  }
  .item {
    padding: 10px;
  }
  .item-icon-left .icon, .OSILineCSS .item-icon-right .icon {
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
    top: 0;
    height: 100%;
    font-size: 14px;
    color: #ddd;
  }
  .item-icon-right .icon {
    right: 0px;
  }

  //搜索按钮
  button {
    outline: none;
    background: transparent;
    border: none;
    border-radius: 10px;
    color: #fff;
    height: 0.69444444rem;
    line-height: 0.69444444rem;
  }
  .searchBlooth {
    position: fixed;
    bottom: 0px;
    background: #3492e9;
    width: 100%;
    height: 40px;
    line-height: 40px;
  }
  .bar.bar-assertive.bar-footer {
    background-image: none;
  }
  .bar.bar-assertive {
    border-color: transparent;
    background-color: transparent;
    background-image: none;
  }
  .bar{
    padding: 0;
  }
  .OSIArear {
    .OSITitle {
      height: 120/@rem;
      line-height: 120/@rem;
      text-align: left;
      background: #f4f4f4;
      color: #1296db;
      padding-left: 10px;
    }
    .OSIList {
      .leftShow {
        display: inline-block;
        .name {
          font-size: 12px;
          color: #999;
        }
        .glassname {
          font-size: 14px;
        }
        span {
          display: block;
        }
      }
      .rightShow {
        display: inline-block;
        float: right;
        span {
          margin-top: 0px;
          display: block;
        }
        .totalTime {
          margin-top: 20px;
          font-size: 12px;
          color: #3492e9;
        }
      }
      ul li {
        .yqdShow {
          background: #83ecad;
          padding: 5px;
          line-height: 35px;
          border-radius: 50%;
          display: inline-block;
          float: left;
          color: #fff;
          margin-right: 10px;
          font-size: 12px;
        }
      }
    }
  }
}