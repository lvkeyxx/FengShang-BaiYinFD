.EleTradCSS {
  * {

  }
  @rem: 144rem;
  background-color: #fff;
  .Eletradbody{
    width: 100%;
    background: #f2f2f2;
  }
  .electricSelect {
    display: inline-block;
    margin-left: 10px;
    color: #000;
    .rightselect{
      background: transparent;
      border:none;
      background: url("../images/PM/selectdown_icon.png")no-repeat right;
      color:#000;
      width:80px;
    }
    .selectOther {
      background: url("../images/PM/selectdown_icon.png");
      width: 15px;
      height: 15px;
      display: inline-block;
      vertical-align: middle;
      margin-left: 15px;
    }
  }
  .marketrbased{
    line-height: 38px;
    height: 38px;
    width: 100%;
    background: #F2F2F2;
    float: right;
    .rft{
      text-align: right;
      font-size: 0.28rem;
      color: #454545;
    }
  }
  .graph {
    background: #fff;
    margin: 0 20/@rem 20/@rem 20/@rem;
    border-radius: 40/@rem;
    padding: 20/@rem;
    .jctIcon {
      background: #ff0000;
    }
    .bdqdIcon {
      background: green;
    }
    .qddeIcon {
      background: #ffee00;
    }
    .dhIcon {
      background: #3492e9;
    }
    .stbIcon{
      background: #e94fd9;
    }
    .graphImg {
      text-align: center;
      height: 300px;
    }
  }
  .graphList {
    background: #fff;
    border-radius: 40/@rem;
    table{
      width: 100%;
    }
    table th {
      padding: 10px 5px;
      background: #e1effc;
      vertical-align: middle;
      color: #3492e9;
    }
    table th:first-child {
      border-top-left-radius: 15px;
      border-right: 1px solid #d7e2ec;
      width: 70px;
    }
    table th:last-child {
      border-top-right-radius: 15px;
    }
    table td {
      border: 1px solid #fefefe;
      border-collapse: collapse;
      padding: 10px 5px;
      text-align: center;
      vertical-align: middle;
      border-bottom: 1px solid #e1effc;
    }
    table tr td:first-child {
      border-left: 1px solid #e1effc;
      border-right: 1px solid #e1effc;
      color: #3492e9;
    }
  }
}