            <h3><span>{{dateTimeShow}}</span>未打卡<span>共{{personNum}}人</span></h3>
            <ul class="list">
                <li class="item item-icon-right" ng-click="towdkDetail(x)" ng-repeat="x in wdkList">
                    <div class="leftShow">
                        <span class="name">{{x.PERSON_NAME}}</span>
                        <span class="glassname">{{partName}}</span>
                    </div>
                    <div class="rightShow">
                        <span class="item-note">{{x.COUNT}}次</span>
                        <i class="icon ion-chevron-right"></i>
                    </div>
                </li>
