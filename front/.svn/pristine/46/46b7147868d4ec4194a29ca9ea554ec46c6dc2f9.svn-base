.OSICSS {
  @rem: 144rem;
  background-color: #fff;
  .item-icon-left .icon, .item-icon-right .icon {
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
    top: 0;
    height: 100%;
    font-size: 14px;
    color: #ddd;
  }
  .item-icon-right {
    padding-right: 34px;
  }
  .item-icon-right .icon {
    right: 0px;
  }
  button {
    outline: none;
    background: transparent;
    border: none;
    border-radius: 10px;
    color: #fff;
    height: 100/@rem;
    line-height: 100/@rem;
  }
  //搜索按钮
  .searchBlooth {
    position: fixed;
    bottom: 0px;
    background: #3492e9;
    width: 100%;
    height: 40px;
    line-height: 40px;
  }
  .bar.bar-assertive.bar-footer {
    background-image: none;
  }
  .bar.bar-assertive {
    border-color: transparent;
    background-color: transparent;
    background-image: none;
  }
  .bar {
    padding: 0;
  }
  .osiList {
    background: #fff;
    //margin: 20/@rem 0/@rem;
    /*border-radius: 40/@rem;*/
    .scanQRcode {
      padding: 5px;
      border-bottom: 1px solid #f4f4f4;
      background: #fff;
      text-align: center;
      display: block;
      text-align: center;
      color: #1296db;
      .scanCode {
        background: url(../images/OSI/scanQRcode_icon.png) no-repeat right;
        display: block;
      }
      .position {
        text-align: left;
        padding: 5px 0;
        .position_icon {
          background: url(../images/attence/position_icon.png) no-repeat center;
          background-size: 100%;
          width: 25px;
          height: 25px;
          display: inline-block;
          vertical-align: middle;
        }
        .qdBtn {
          float: right;
          display: inline-block;
          width: 50px;
          height: 30px;
          line-height: 30px;
          background: #3492e9;
          transform: translateY(-10/@rem);
        }
      }
