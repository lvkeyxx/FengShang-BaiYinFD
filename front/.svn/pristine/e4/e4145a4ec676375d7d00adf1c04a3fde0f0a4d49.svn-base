.powerCSS {
  @rem: 144rem;
  background-color: #f4f4f4;
  .tabs-positive > .tabs, .tabs.tabs-positive {
    border-color: #fff;
    background-color: #fff;
    background-image: linear-gradient(0deg, #0c60ee, #0c60ee 50%, transparent 50%);
    color: #000;
  }
  .my_dc {
    background: none;
    border: 1px solid #36a2ef;
    cursor: pointer;
    height: 20px;
    width: 50px;
    color: #36a2ef;
    position: absolute;
    right:60/@rem;
    top:120/@rem;
    border-radius: 3px;
    font-size: 12px;
  }
  .yue_dc{
    background: none;
    border: 1px solid #36a2ef;
    cursor: pointer;
    height: 20px;
    width: 50px;
    color: #36a2ef;
    position: absolute;
    right:220/@rem;
    top: 120/@rem;
    border-radius: 3px;
    font-size: 12px;
  }
  .pane, .view {
    position: absolute;
    top: 100px;
    height: 100%;
    background-color: #f4f4f4;
  }
  .tab-item.tab-item-active {
    opacity: 1;
    color: #3492e9;
    border-bottom: 3px solid #3492e9;
  }
  .tab-item.tab-item-active span {
    color: #3492e9;
  }
  .loadList {
    /*margin-bottom: 110px;*/
    .dayList {
      background: #fff;
      border-radius: 40/@rem;
      margin: 0 20/@rem 20/@rem 20/@rem;
      /*padding: 20/@rem;*/
      .iconbtn {
        display: block;
        .yearIcon {
          background: url("../images/power/day_power_icon.png") no-repeat;
          width: 45px;
          height: 41px;
          display: block;
          float: left;
        }
        .dltbBtn {
          float: right;
          background: #3492e9;
          color: #fff;
          border-top-left-radius: 0/@rem;
          border-top-right-radius: 40/@rem;
          border-bottom-right-radius: 0/@rem;
          border-bottom-left-radius: 60/@rem;
          border: none;
          width: 120/@rem;
          height: 120/@rem;
        }
        .dltbBtn .dltbicon {
          background: url("../images/power/fillForm_icon.png") no-repeat;
          width: 27px;
          height: 27px;
          display: block;
          margin: 0 auto;
        }
      }
      .litop {
        padding: 40/@rem;
        .relativeRatio {
          //text-align: right;
          line-height: 80/@rem;
        }
        .powertime{

        }
        .powerright{
          float: right;
        }
        .relativeRatio em {
          color: #3492e9;
        }
      }
      .litop ul {
        font-size: 0;
        border-bottom: 2px solid #3492e9;
      }
      .litop ul li {
        font-size: 40/@rem;
        display: inline-block;
        text-align: center;
      }

      .allshow {
        padding: 0 30/@rem;
        border-bottom: 2px solid #3492e9;
        .allshowul {
          font-size: 0;
        }
        .allshowul li {
          font-size: 60/@rem;
          display: inline-block;
          text-align: center;

        }
        .allshowul .num {
          color: #3492e9;
          text-align: right;
        }
        .allshowul .unit {
          color: #000;
        }
      }
      .lifoot {
        padding: 20/@rem;
        .dayequally {
          font-size: 0;
        }
        .dayequally li {
          font-size: 40/@rem;
          display: inline-block;
          text-align: center;
        }
        .dayequally .num {
          color: #3492e9;
        }
      }
    }
