<ion-view class="OSIHistoryDetailCSS">
    <ion-nav-title>巡查节点</ion-nav-title>
    <ion-content has-bouncing="false" scrollbar-y="false">
        <div class="OSIDetail">
            <div class="OSITitle">
                <div class="osiLeft">
                    <span>{{description}}({{inspectType}})</span>
                    <span class="osiTime">{{startTime}} ~  {{endTime}}</span>
                </div>
            </div>
            <div class="OSIList">
                <ul class="list">
                    <li class="item item-icon-right" ng-click="toOSIArear()" ng-repeat="h in hDetail">
                        <div class="leftShow">
                            <i ng-style="identify">{{identifyShow}}</i>
                            <span class="name">{{h.ADDRESS}}</span>
                            <span class="name">{{h.MCH_NAME}}</span>
                            <span class="glassname">{{h.RECORD_TIME}}</span>
                        </div>
                    </li>
