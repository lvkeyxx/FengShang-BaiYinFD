.facilityInfoCSS {
  @rem: 144rem;
  background-color: #fff;
  .facilityList {
    background: #fff;
    .scanQRcode {
      padding: 5px;
      border-bottom: 1px solid #f4f4f4;
      background: #fff;
      text-align: center;
      display: block;
      text-align: center;
      color: #1296db;
      .scanCode {
        background: url(../images/OSI/scanQRcode_icon.png) no-repeat right;
        display: block;
      }
    }
  }
  .tab-box {
    font-size: 0;
    border-top: 10px solid #f4f4f4;
  }

  .tab-box input {
    display: none;
  }

  .tab-box label {
    display: inline-block;
    text-align: center;
    width: 50%;
    color: #000;
    font-size: 14px;
    line-height: 50px;
    border-bottom: 2px solid #ccc;
  }
  .tab-content {
    font-size: 12px;
    margin-top: 5px;
    display: none;
    ul li {
      border-bottom: 1px dotted #f4f4f4;
      padding: 10px;
      h3,span{
        padding: 0px 10px;
      }
      table {
        width: 100%;
        margin: 10px 0px;
      }
      table caption{
        background: #e1effc;
        height: 35px;
        line-height: 35px;
        color:#3492e9;
      }
      table th {
        padding: 10px 5px;
        /*background: #e1effc;*/
        vertical-align: middle;
        color: #3492e9;
        border-right: 1px solid #d7e2ec;
        text-align: center;
      }
      table th:first-child {
        /*border-top-left-radius: 15px;*/
        border-right: 1px solid #d7e2ec;
        border-left: 1px solid #d7e2ec;
        width: 50px;
      }
      table th:nth-child(2) {

      }
      table th:last-child {
        /*border-top-right-radius: 15px;*/
        border-right: 1px solid #d7e2ec;
      }
      table tr td:first-child {
        border-left: 1px solid #e1effc;
        border-right: 1px solid #e1effc;
        color: #3492e9;
      }
      table tr td:last-child {
        border-right: 1px solid #e1effc;
      }
      table td {
        border: 1px solid #e1effc;
        border-collapse: collapse;
        padding: 10px 5px;
        text-align: center;
        vertical-align: middle;
        border-bottom: 1px solid #e1effc;
        span {
          display: block;
          text-align: center;
        }
      }
    }

  }
  .tab-box input:nth-of-type(1):checked ~ .tab-contents .tab-content:nth-of-type(1),
  .tab-box input:nth-of-type(2):checked ~ .tab-contents .tab-content:nth-of-type(2),
  .tab-box input:nth-of-type(3):checked ~ .tab-contents .tab-content:nth-of-type(3) {

    display: block;

  }

  .tab-box input:nth-of-type(1):checked ~ .tab-box-label label:nth-of-type(1),
  .tab-box input:nth-of-type(2):checked ~ .tab-box-label label:nth-of-type(2),
  .tab-box input:nth-of-type(3):checked ~ .tab-box-label label:nth-of-type(3) {

    background-color: transparent;
    border-bottom: 2px solid #3492e9;
    color: #3492e9;

  }
}