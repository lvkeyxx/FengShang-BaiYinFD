angular.module('BaiYin.common.dateFilter', [])

.filter('myFilter',[function(){
    return function(input){
    	var arr1=[];
    	arr1=input;
        var out='';
        var arr=[];
       arr=arr1.split(' ')[0].split('-');
      out=arr[0]+"年"+arr[1]+'月'+arr[2]+'日';
        return out;
    }

}])
