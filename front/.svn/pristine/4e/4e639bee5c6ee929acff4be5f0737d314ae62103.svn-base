.OSICSS {
  @rem: 144rem;
  background-color: #fff;
  button{
    outline: none;
    background: transparent;
    border: none;
    border-radius: 10px;
    color: #fff;
    height: 100/@rem;
    line-height: 100/@rem;
  }
  //搜索按钮
  .searchBlooth{
    position: fixed;
    bottom:0px;
    background: #3492e9;
    width: 100%;
    height: 40px;
    line-height: 40px;
  }
  .bar.bar-assertive.bar-footer {
    background-image: none;
  }
  .bar.bar-assertive {
    border-color: transparent;
    background-color: transparent;
    background-image: none;
  }
  .bar{
    padding: 0;
  }
  .osiList {
    background: #fff;
    //margin: 20/@rem 0/@rem;
    /*border-radius: 40/@rem;*/
    .selectMounth {
      padding: 5px;
      border-bottom: 1px solid #f4f4f4;
      background: #f4f4f4;
      text-align: center;
      display: block;
      text-align: center;
      color: #1296db;
      .selectDateBtn {
        margin: 0 auto;
        border: none;
        text-align: left;
        width: 100px;
        display: block;
        background: transparent;
        color: #1296db;
        background: url(../images/PM/selectDowm_blueicon.png) no-repeat right;
      }
      /*.leftSelect{
        background: url("../images/attence/leftIcon.png");
        width: 25px;
        height: 25px;
        display: inline-block;
        margin-right: 10px;
        vertical-align: middle;
      }
      .rightSelect{
        background: url("../images/attence/rightIcon.png");
        width: 25px;
        height: 25px;
        display: inline-block;
        margin-left: 10px;
        vertical-align: middle;
      }*/
    }
    .zrqyTable {
      .signInBtn{
        background: #51bf07;
        width: 50px;
      }
      .yqdBg{
        background: #ccc;
      }
      padding: 10px;
      table {
        width: 100%;
      }
      table th {
        padding: 10px 5px;
        background: #e1effc;
        vertical-align: middle;
        color: #3492e9;
      }
      table th:first-child {
        border-top-left-radius: 15px;
        border-right: 1px solid #d7e2ec;
        width: 80px;
      }
      /*table th:nth-child(2) {
        width: 100px;
      }*/
      table th:last-child {
        border-top-right-radius: 15px;
      }
      table tr td:first-child {
        border-left: 1px solid #e1effc;
        border-right: 1px solid #e1effc;
        color: #3492e9;
      }
      table tr td:last-child {
        border-right: 1px solid #e1effc;
      }
      table td {
        border: 1px solid #fefefe;
        border-collapse: collapse;
        padding: 10px 5px;
        text-align: center;
        vertical-align: middle;
        border-bottom: 1px solid #e1effc;
        span {
          display: block;
          text-align: center;
        }
      }
    }
