.OSIDetailCSS {
  @rem: 144rem;
  background-color: #fff;
  .OSIList {
    background: #fff;
    padding: 0 10px 10px 10px;
  }
  .OSIDetail {
    .OSITitle{
      height: 120/@rem;
      line-height: 120/@rem;
      text-align: center;
      background: #fff;
    }
    table {
      width: 100%;
    }
    table th {
      padding: 10px 5px;
      background: #e1effc;
      vertical-align: middle;
      color: #3492e9;
    }
    table th:first-child {
      border-top-left-radius: 15px;
      border-right: 1px solid #d7e2ec;
      width: 120px;
    }
    table th:nth-child(2) {
      width: 150px;
    }
    table th:last-child {
      border-top-right-radius: 15px;
    }
    table tr td:first-child {
      border-left: 1px solid #e1effc;
      border-right: 1px solid #e1effc;
      color: #3492e9;
    }
    table tr td:last-child {
      border-right: 1px solid #e1effc;
    }
    table td {
      border: 1px solid #fefefe;
      border-collapse: collapse;
      padding: 10px 5px;
      text-align: center;
      vertical-align: middle;
      border-bottom: 1px solid #e1effc;
      span {
        display: block;
        text-align: center;
      }
    }
  }

}