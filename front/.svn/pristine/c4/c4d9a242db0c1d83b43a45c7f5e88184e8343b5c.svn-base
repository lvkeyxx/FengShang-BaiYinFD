.countAttenceCSS {
  @rem: 144rem;
  background-color: #fff;
  .ydNum{
    text-align: right;
    margin-right: 10px;
  }
  .selectMounth {
    border-top: 10px solid #f4f4f4;
    padding: 10px 5px 10px 5px;
    border-bottom: 1px solid #f4f4f4;
    text-align: center;
    .leftSelect{
      margin-left: 70px;
      background: url("../images/attence/leftIcon.png");
      width: 25px;
      height: 25px;
      display: inline-block;
      margin-right: 10px;
      vertical-align: middle;
    }
    .rightSelect{
      background: url("../images/attence/rightIcon.png");
      width: 25px;
      height: 25px;
      display: inline-block;
      margin-left: 10px;
      vertical-align: middle;
    }
  }
  .selectGlass{
    display: inline-block;
    float: right;
    transform: translateY(-20/@rem);
    input{
      border:none;
      width: 100px;
      border: none;
      background: url(../images/PM/selectDowm_blackicon.png) no-repeat right;
      display: inline-block;
    }
  }
  //饼图
  .countResult{
    margin-top: 20px;
    text-align: center;
    border-bottom: 1px solid #f4f4f4;
    padding-bottom: 20px;
    .pieShow{
      text-align: center;
      display: inline-block;
      margin-left: 70px;
      img{
        width: 300/@rem;
        height: 300/@rem;
      }
    }
    .pieColor{
      transform: translateY(50/@rem);
      display: inline-block;
      float: right;
      ul li{
        padding: 10px;
        .sdColor{
          width: 100/@rem;
          height: 50/@rem;
          background: #07a70b;
          display: inline-block;
          vertical-align: sub;
          margin-left: 10px;
        }
        .wdColor{
          width: 100/@rem;
          height: 50/@rem;
          background:#ff0000;
          display: inline-block;
          vertical-align: sub;
          margin-left: 10px;
        }
      }
    }
  }
  //统计数据
  .countNum{
    margin-top: 10px;
    ul{
      font-size: 0;
    }
    ul li{
      font-size: 14px;
      display: inline-block;
      padding: 10px 10px;
      height: 100/@rem;
      line-height: 100/@rem;
      i{
        width: 80px;
        margin-left: 10px;
        margin-right: 10px;
        height: 50/@rem;
        display: inline-block;
        vertical-align: middle;
      }
      .ydIcon{
        background: #3492e9;
      }
      .sdIcon{
        background: #1BBB37;
      }
      .cdIcon{
        background: #ffee00;
      }
      .wdIcon{
        background: #ff0000;
      }
    }
  }
  .tips{
    padding: 10px 10px 0px 15px;
    color: #ff0000;
    margin-top: 10px;
  }
}