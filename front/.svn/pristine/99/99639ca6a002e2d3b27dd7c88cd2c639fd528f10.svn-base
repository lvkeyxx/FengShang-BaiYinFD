<ion-view class="EleTradCSS">
    <ion-nav-title>结算电量</ion-nav-title>
    <ion-content has-bouncing="false" scrollbar-y="false">
        <div class="Eletradbody">
            <div class="electricSelect">
                <input type="text" class="rightselect" contenteditable="true" id="setEleTradID" readonly ng-click="selectsetEletrad()">
            </div>
        </div>
        <div class="graph">
            <div class="graphImg">
                <powerline id="setEleTradst" legend="legend" item="item" data="data">
                </powerline>
            </div>
        </div>
        <div class="marketrbased clearfix">
            <div class="rft">单位：万千瓦时、元/兆瓦时</div>
        </div>
        <div class="graphList">
            <table>
                <tr>
                    <th>日期</th>
                    <th>结算电量</th>
                    <th>结算电价</th>
                    <th>市场化电量</th>
                    <th>市场化电价</th>
                    <th>基础电量</th>
                    <th>基础电价</th>
                </tr>
                <tr >
                    <td>2018-05-22</td>
                    <td>41.62<!--{{gsvalue.DAY_ELE_AMOUNT | number:2}} ng-repeat="gsvalue in gsCompanyPower track by $index"--></td>
                    <td>140</td>
                    <td>32.99</td>
                    <td>163</td>
                    <td>32.99</td>
                    <td>163</td>
                </tr>
            </table>
        </div>
    </ion-content>
</ion-view>

