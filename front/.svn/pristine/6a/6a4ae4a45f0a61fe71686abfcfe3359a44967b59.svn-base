<ion-view class="cdListCSS">
    <ion-nav-title>正常</ion-nav-title>
    <ion-content has-bouncing="false" scrollbar-y="false">
        <div class="content">
            <h3><span>{{dateTimeShow}}</span>正常<span>共{{personNum}}人</span></h3>
            <ul class="list">
                <li class="item item-icon-right" ng-click="towdDetail(x)" ng-repeat="x in ztList">
                    <div class="leftShow">
                        <span class="name">{{x.PERSON_NAME}}</span>
                        <span class="glassname">{{partName}}</span>
                    </div>
                    <div class="rightShow">
                        <span class="item-note">{{x.COUNT}}次</span>
                        <i class="icon ion-chevron-right"></i>
                    </div>
                </li>
            </ul>
        </div>
    </ion-content>
</ion-view>