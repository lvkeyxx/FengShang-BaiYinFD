    transform: translateY(50/@rem);
  }
  .kqqd_img {
    display: inline-block;
    background: url(../images/PM/kqqd_img_icon_grey.png) no-repeat center;
    background-size: 100%;
    width: 80/@rem;
    height: 80/@rem;
    transform: translateY(50/@rem);
  }
  .rwgl_img {
    display: inline-block;
    background: url(../images/PM/rwgl_img_icon_grey.png) no-repeat center;
    background-size: 100%;
    width: 80/@rem;
    height: 80/@rem;
    transform: translateY(50/@rem);
  }
  .sbxx_img {
    display: inline-block;
    background: url(../images/PM/sbxx_img_icon_grey.png) no-repeat center;
    background-size: 100%;
    width: 80/@rem;
    height: 80/@rem;
    transform: translateY(50/@rem);
  }
  .xcxc_img {
    display: inline-block;
    background: url(../images/PM/xcxc_img_icon_grey.png) no-repeat center;
    background-size: 100%;
    width: 80/@rem;
    height: 80/@rem;
    transform: translateY(50/@rem);
  }
  .xctj_img {
    display: inline-block;
    background: url(../images/PM/xctj_img_icon_grey.png) no-repeat center;
    background-size: 100%;
    width: 80/@rem;
    height: 80/@rem;
    transform: translateY(50/@rem);
  }
  .sczb_img {
    display: inline-block;
    background: url(../images/PM/sczbIcon_img.png) no-repeat center;
    background-size: 100%;
    width: 80/@rem;
    height: 80/@rem;
    transform: translateY(50/@rem);
  }
  //广告显示
  .advert {
    margin-top: 5px;
  }
  .advert img {
    width: 100%;
    height: 1.2rem;
  }
  .allbg {
    background: rgba(0, 0, 0, 0.5);
    width: 100%;
    height: 100%;
    position: absolute;
    top:90/@rem;
    z-index: 99;
  }
  .otherList{
    background: #fff;
    position: fixed;
    z-index: 9999;
    top:90/@rem;
    width: 100%;
    height: 100px;
    overflow: scroll;
  }
  .otherList ul li{
    color: #3492e9;
    line-height: 100/@rem;
    text-align: center;
  }
}