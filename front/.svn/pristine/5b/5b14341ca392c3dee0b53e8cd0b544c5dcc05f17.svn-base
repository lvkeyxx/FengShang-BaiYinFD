.wordStyle {
    height: 32px;
    line-height: 32px;
    color: #fff;
    text-decoration: none;
    padding: 0 6px;
    font-size: .26rem;
}


/*footer样式*/

.footStyle {
    height: 1rem;
    line-height: 1rem;
    background: -o-linear-gradient(#3c82c1 5%, #025cad);
    background: -ms-linear-gradient(#3c82c1 5%, #025cad);
    background: linear-gradient(#3c82c1 5%, #025cad);
    background: -moz-linear-gradient(top, #3c82c1, #025cad);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#3c82c1), to(#025cad));
    .button {
        margin: 0 auto;
        min-width: 2.45rem;
        color: #fff;
        font-size: .28rem;
        background: none;
    }
}
.ionic_datepicker_popup .popup-buttons button{
    background-color: #3492e9;
}
.ionic_datepicker_popup .popup-body {
    .selected_date_full {
        background: #ed762f!important;
    }
    .button-clear {
        color: #ed762f!important;
    }
    .month_select {
        border-bottom: 1px solid #ed762f!important;
        select {
            width: 100%!important;
            direction: ltr!important;
        }
    }
    .year_select:after {
