.agentsView {
    @rem: 144rem;
    font-size: 14px;
    background-color: #EEEEEE;
    .allCssDate {
        display: flex;
        justify-content: space-between;
        border-bottom: 1px solid #cccccc;
    }
    .padding {
        padding: 0 45/@rem ;
    }
    .allCss {
        border-bottom: 1px solid #cccccc;
    }
    .myCss {
        width: 40%;
    }
    .lastDate {
        display: flex;
        justify-content: space-between;
        padding: .4rem 0;
    }
    .accessoryCss {
        padding-left: 15/@rem;
        line-height: 111/@rem;
        background: url(../images/button.png) no-repeat center;
        background-size: 100%;
        color: white;
        border-radius: 20/@rem;
        margin: 41/@rem 0;
    }
    .fromIn {
        display: flex;
        justify-content: space-between;
    }
    .addMessage {
        padding: 0 30px;
    }
    .beizhu {
        border-bottom: 1px solid #cccccc;
    }
    .approvalCss {
        margin: 42/@rem 0 10/@rem 0;
    }
    .lineCss {
        width: 100%;
        height: 1px;
        background-color: #cccccc;
    }
    .riseCss {
        margin-top: 41/@rem;
    }
    .explainCss {
        padding: 21/@rem 0;
    }
    .inputCss {
        overflow-x: hidden;
        overflow-y: visible;
        width: 100%;
        background-color: white;
        border-radius: 10/@rem;
        padding: .2rem;
        resize: none;
    }
    .buttonCss {
        margin-top: 16/@rem;
        display: flex;
        justify-content: space-around;
    }
    .button {
        margin: 25/@rem;
        width: 315/@rem;
        height: 109/@rem;
        color: white;
    }
    .back1 {
        background-color: #009933;
    }
    .back2 {
        background-color: #cc0000;
    }
    .back3 {
        background-color: #999999;
    }
    .form_img {
        padding: .2rem 0rem;
        span {
            display: inline-block;
            margin-bottom: .15rem;
        }
        .pictures {
            .scroll {
                height: 1.2rem;
                img {
                    width: 1.2rem;
                    height: 1.2rem;
                }
                ul {
                    display: inline-flex;
                }
                li.itemPicture {
                    float: left;
                    margin-left: .1rem;
                    width: 1.2rem;
                }
                .caremaImg {
                    margin-left: 0.1rem;
                    background: url(../images/addCarema.png) no-repeat;
                    width: 1.2rem;
                    height: 1.2rem;
                    background-size: 98%;
                    float: left;
                    margin-top: 1.3px;
                }
            }
        }
        p {
            padding-top: 0.3rem;
        }
    }
    .timeInp {
        width: 2.5rem;
        height: .6rem;
        background-color: white;
        display: inline-block;
        vertical-align: middle;
        text-align: center;
        line-height: .6rem;
        margin-left: .24rem;
        border-radius: .1rem;
    }

}