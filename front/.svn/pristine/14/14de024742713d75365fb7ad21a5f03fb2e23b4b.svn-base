<ion-view class="wdkDetailCSS">
    <ion-nav-title>打卡详情</ion-nav-title>
    <ion-content has-bouncing="false" scrollbar-y="false">
        <div class="content">
            <h3><span>{{dateTimeShow}}</span><em>{{personName}}</em><!--<span>未打卡</span><span>共{{count}}次</span>--></h3>
            <div class="myAttencelist">
                <table>
                    <tr>
                        <th>日期</th>
                        <th>打卡时间</th>
                        <th>备注</th>
                    </tr>
                    <tbody ng-repeat="x in wdkPersonList">
                    <tr ng-repeat="wdkitem in x.list track by $index">
                        <td ng-if="$index == 0" rowspan="{{x.list.length}}" class="countdate">
                            {{x.month}}/{{x.date}}
                            <span>{{x.week}}</span>
                        </td>
                        <td class="{{wdkitem.confirmDate.length>0 ? 'abnormalgreen' : 'abnormalAttence'}}">{{wdkitem.actualStartTime}}-{{wdkitem.actualEndTime}}</td>
                        <td ng-bind-html="wdkitem.remarks" class="{{wdkitem.confirmDate.length>0 ? 'abnormalgreen' : 'abnormalAttence'}} coustvalueright"></td>
                    </tr>
                    </tbody>
                </table>
